{
  "name": "@nodepy/registry",
  "version": "0.0.5-dev",
  "license": "MIT",
  "description": "the nodepy package registry",
  "repository": "https://github.com/nodepy/registry",
  "engines": {
    "nodepy": ">=0.0.6",
    "python": ">=2.7.0 || >=3.3.0"
  },
  "dependencies": {
    "@nodepy/pm": "-e _vendor/nodepy/nodepy-pm",
    "@nodepy/werkzeug-reloader-patch": "-e _vendor/werkzeug-reloader-patch",
    "@nodepy/spdx-licenses": "-e _vendor/spdx-licenses"
  },
  "python-dependencies": {
    "Flask": ">=0.12",
    "Flask-HTTPAuth": ">=3.2.2",
    "Flask-RESTful": ">=0.3.5",
    "PyYAML": ">=3.12",
    "Pygments": ">=2.2.0",
    "libsass": ">=0.12.3",
    "markdown": ">=2.6.8",
    "mkdocs": ">=0.16.1",
    "mongoengine": ">=0.11.0"
  },
  "extensions": [
    "@nodepy/werkzeug-reloader-patch"
  ]
}
